    "/api/DateTime/AvailableDate": {
      "post": {
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": [
          "DateTime"
        ],
        "summary": "Получить даты доступности",
        "description": "Получает ближайшие доступные даты доставки и самовывоза по каждому товару",
        "requestBody": {
          "description": "содержимое отправляемого запроса",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/payload_availabledate"
              },
              "examples": {
                "Даты доступности без учета количества (листинг и т.д.)": {
                  "$ref": "#/components/examples/availabledate_basic"
                },
                "Даты доступности с учетом количества (корзина)": {
                  "$ref": "#/components/examples/availabledate_quantity"
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "запрос выполнен",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "type": "object",
                      "additionalProperties": {
                        "type": "object",
                        "description": "Ключ - артикул",
                        "properties": {
                          "code": {
                            "type": "string",
                            "description": "Артикул"
                          },
                          "sales_code": {
                            "type": "string",
                            "description": "Код уценки"
                          },
                          "courier": {
                            "type": "string",
                            "format": "date-time",
                            "description": "Дата курьерской доставки"
                          },
                          "self": {
                            "type": "string",
                            "format": "date-time",
                            "description": "Дата самовывоза"
                          }
                        }
                      }
                    }
                  }
                },
                "example": {
                  "data": {
                    "101937": {
                      "code": "101937",
                      "courier": "2021-03-23T00:00:00",
                      "self": "2021-03-24T00:00:00"
                    },
                    "1020938": {
                      "code": "1020938",
                      "courier": "2021-03-24T00:00:00",
                      "self": "2021-03-24T00:00:00"
                    },
                    "789734_2242670": {
                      "code": "789734",
                      "sales_code": "2242670",
                      "courier": "2021-05-05T00:00:00",
                      "self": "2021-05-06T00:00:00"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "некорректные входные данныу",
            "content": {}
          },
          "401": {
            "description": "Не аутентифицирован"
          },
          "403": {
            "description": "Отсутствуют права на данный эндпоинт"
          },
          "500": {
            "description": "внутренняя ошибка сервера",
            "content": {}
          }
        }
      }
    },
    "/api/Authenticate/login": {
      "post": {
        "tags": [
          "Authenticate"
        ],
        "requestBody": {
          "content": {
            "application/json": {
