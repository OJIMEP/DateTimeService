    "/api/DateTime/IntervalList": {
      "post": {
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": [
          "DateTime"
        ],
        "summary": "Получить интервалы доставки",
        "description": "Получает список доступных интервалов доставки",
        "requestBody": {
          "description": "содержимое отправляемого запроса",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/payload_interval"
              },
              "examples": {
                "Интервалы для курьерской доставки по коду адреса": {
                  "$ref": "#/components/examples/interval_courier"
                },
                "Интервалы для курьерской доставки по координатам": {
                  "$ref": "#/components/examples/interval_courier_coords"
                },
                "Интервалы для самовывоза с ПВЗ": {
                  "$ref": "#/components/examples/interval_self"
                },
                "Интервалы по имеющемуся заказу (Creatio)": {
                  "$ref": "#/components/examples/interval_byOrder"
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "запрос выполнен",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "properties": {
                          "begin": {
                            "type": "string",
                            "format": "date-time",
                            "description": "Начало интервала"
                          },
                          "end": {
                            "type": "string",
                            "format": "date-time",
                            "description": "Конец интервала"
                          },
                          "bonus": {
                            "type": "string",
                            "format": "boolean",
                            "description": "Признак того, что за выбор этого интервала будет больше бонусов"
                          }
                        }
                      },
                      "example": [
                        {
                          "begin": "2021-03-23T19:00:00",
                          "end": "2021-03-23T21:00:00",
                          "bonus": true
                        },
                        {
                          "begin": "2021-03-23T21:00:00",
                          "end": "2021-03-23T23:00:00",
                          "bonus": false
                        }
                      ]
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "некорректные входные данныу",
            "content": {}
          },
          "401": {
            "description": "Не аутентифицирован"
          },
          "403": {
            "description": "Отсутствуют права на данный эндпоинт"
          },
          "500": {
            "description": "внутренняя ошибка сервера",
            "content": {}
          }
        },
        "x-codegen-request-body-name": "body"
      }
    },
