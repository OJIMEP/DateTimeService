            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Удачная аутентификация, возвращается токен",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string",
                      "description": "Токен аутентификации для помещения в заголовок Authorization",
                      "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..s5HYYpZBF5POynoMYPDM1s10ti4aBc14MTu08i89ok4"
                    },
                    "expiration": {
                      "type": "string",
                      "format": "date-time",
                      "description": "Срок действия токена в UTC",
                      "example": "2021-03-24T13:24:25Z"
                    },
                    "refresh": {
                      "type": "string",
                      "description": "Refresh токен",
                      "example": "PUo3hQD2YR5J2qeDS/EUfep5LAz3uPod6W70m/zmqf0y8ZZThtvKJb6Ft6fqkf/I+Vw7moD8i1i3OEaPhB/PgQ=="
                    },
                    "expiration_refresh": {
                      "type": "string",
                      "format": "date-time",
                      "description": "Срок действия refresh-токена в UTC",
                      "example": "2021-04-08T08:01:51.979274Z"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Неудачная аутентификация",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "description": "Подробности",
                      "example": "Invalid token"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
